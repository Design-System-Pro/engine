{
  "name": "ds-project-plugin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "main": "vite build -c ./vite.config.plugin.ts",
    "ui": "tsc && vite build --minify esbuild",
    "build": "concurrently -n main,ui \"npm run main\" \"npm run ui\"",
    "watch": "concurrently -n main,ui \"npm run main -- --watch\" \"npm run ui -- --watch\"",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "rambda": "^9.2.1",
    "style-dictionary": "^4.0.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.58.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "vite-plugin-singlefile": "^0.13.5",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.15",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
